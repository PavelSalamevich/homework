package BoxValue;

public class Box {

    public double length;
    public double width;
    public double height;


    public Box(double length, double width, double height) {
        this.length = length;
        this.width = width;
        this.height = height;


        double v = length * width * height;
        System.out.print("Box value is: " + v + " sm3");
        System.out.println();

    }

    public void print() {
        String result = "Length = " + length +" sm"+ ", Width = " + width +" sm"+ ", Height = " + height +" sm";
        System.out.println(result);
    }
}





package HomeWork;

import BoxValue.Box;

public class HomeWorkConstBox {
    public static void main(String[] args) {

        Box box1 = new Box(2.5, 10, 3.7);
        box1.print();


    }


}
